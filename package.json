{
  "name": "node-express-api-ts",
  "description": "Simple api sample in Node",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --exec 'npm run build && npm run start'",
    "build": "tsc",
    "genkit:dev": "genkit start -- tsx dist/ai/dev.js",
    "genkit:watch": "genkit start -- tsx --watch dist/ai/dev.js",
    "start": "node dist/index.js"
  },
