  "license": "Apache-2.0",
  "dependencies": {
    "@genkit-ai/core": "^1.12.0",
    "@genkit-ai/googleai": "^1.12.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "firebase": "^11.8.1",
    "genkit": "^1.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "genkit-cli": "^1.8.0",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.5"
  }
}
